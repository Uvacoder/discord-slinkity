    input:hover+svg,
    input:checked+svg,
    input:focus+svg {
        opacity: 1;
    }

    input[type="radio"] {
        position: absolute;
        opacity: 0;
        cursor: pointer;
    }

    input[type="text"] {
        background-color: transparent;
        border-bottom: 2px black solid;
    }

    input::placeholder {
        color: rgba(0, 0, 0, 0.26);
    }

    #smiles {
        transform: perspective(2000px) rotateX(40deg) rotateY(0deg) scale(1) translateY(50px);
        opacity: 0;
        transition: all 400ms ease-in-out;
        pointer-events: none;
    }

    #smiles.active {
        transform: perspective(2000px) rotateX(0deg) rotateY(0deg) scale(1) translateY(0px);
        opacity: 1;
        pointer-events: all;
    }

    #smilesContainer {
        pointer-events: none;
    }
</style>

<script>
    var currentTab = 0; // Current tab is set to be the first tab (0)
    function showTab(n) {
        // This function will display the specified tab of the form...
        var x = document.getElementsByClassName("tab");
        x[n].style.display = "flex";
        //... and fix the Previous/Next buttons:
        if (n == 0) {
            document.getElementById("prevBtn").style.display = "none";
            document.getElementById("nextBtn").style.display = "none";
        } else {
            document.getElementById("prevBtn").style.display = "inline";
            document.getElementById("nextBtn").style.display = "inline";
        }
        if (n == (x.length - 1)) {
            document.getElementById("nextBtn").innerHTML = "Submit";
            document.getElementById("nextBtn").setAttribute("type", "submit");
        } else {
            document.getElementById("nextBtn").innerHTML = "Next";
            document.getElementById("nextBtn").setAttribute("type", "button");
        }
    }

    function nextPrev(n) {
        // This function will figure out which tab to display
        var x = document.getElementsByClassName("tab");
        // Exit the function if any field in the current tab is invalid:
        if (n == 1 && !validateForm()) return false;
        // Hide the current tab:
        x[currentTab].style.display = "none";
        // Increase or decrease the current tab by 1:
        currentTab = currentTab + n;
        // if you have reached the end of the form...
        // Otherwise, display the correct tab:
        showTab(currentTab);
        goodvsbad();
    }

    function goodvsbad() {
        if (document.querySelector('input[class = "good"]:checked')) {
            document.getElementById("nt").style.display = "none";
        } else if (document.querySelector('input[class = "bad"]:checked')) {
            document.getElementById("nt").style.display = "inline";
        }
    }

    function validateForm() {
        // This function deals with validation of the form fields
        var x, y, i, valid = true;
        x = document.getElementsByClassName("tab");
        y = x[currentTab].getElementsByTagName("input");
        // A loop that checks every input field in the current tab:
        for (i = 0; i < y.length; i++) {
            // If a field is empty...
            if (y[i].value == "") {
                // add an "invalid" class to the field:
                y[i].className += " invalid";
                // and set the current valid status to false
                valid = false;
            }
        }
        // If the valid status is true, mark the step as finished and valid:
        return valid; // return the valid status
    }

    function smilesSuccess() {
        (data) => console.log(data)
        x = document.getElementsByClassName("tab");  // Find the elements
        for (var i = 0; i < x.length; i++) {
            x[i].style.display = "none";    // Change the content
        };
        document.getElementById("prevBtn").style.display = "none";
        document.getElementById("nextBtn").style.display = "none";
        document.getElementById("thankyouMsg").style.display = "block";
    }

    $("#smilesForm").submit(function () {
        jQuery.ajax({
            method: 'POST',
            data: $(this).serialize(),
            url: 'https://formsubmit.co/ajax/smiles-report@moonface.digital',
            dataType: 'json',
            accepts: 'application/json',
            success: smilesSuccess(),
            error: (err) => console.log(err)
        })
        return false;
    }
    );

    $('#closeSmilesContainer').on('click', function () {
        var data = sessionStorage.getItem('shownSmiles');
        document.getElementById('smilesContainer').style.display = 'none';
        if (data == undefined) {
            sessionStorage.setItem('shownSmiles', 'true');
            //close the pop
        }
    })

    window.addEventListener('load', function () {
        var data = sessionStorage.getItem('shownSmiles');
        if (data) {
            //close the pop
            document.getElementById('smilesContainer').style.display = 'none';
        }
        else {
            showTab(currentTab); // Display the current tab
            document.getElementById('smilesContainer').style.display = 'block';
        }
    });

    window.addEventListener('scroll', () => {
        if (window.scrollY >= 800) {
            document.getElementById('smiles').classList.add("active");
        }
    });

</script>